<Window x:Class="RSS_Shell.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prsm="http://www.codeplex.com/prism"
        Title="Simple WPF RSS Reader" Height="450" Width="800">
    <DockPanel>
        <ItemsControl DockPanel.Dock="Left" Name="TreeRegion" prsm:RegionManager.RegionName="TreeRegion"/>
        <ItemsControl Name="MainRegion" prsm:RegionManager.RegionName="MainRegion"/>
    </DockPanel>
</Window>